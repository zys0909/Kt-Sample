<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.group.dev">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission-sdk-23 android:name="android.permission.CALL_PHONE" />

    <application
        android:name="com.group.dev.SampleApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning"
        tools:targetApi="n">
        <activity
            android:name="com.group.dev.main.MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity-alias
            android:name=".app.App"
            android:icon="@color/colorAccent"
            android:targetActivity="com.group.dev.main.MainActivity" />

        <activity android:name="com.group.dev.ui.qmhome.StickyTop2Activity" />
        <activity android:name="com.group.dev.ui.wanandroid.WanMainActivity" />
        <activity android:name="com.group.dev.ui.wanandroid.ArticleListActivity" />
        <activity android:name="com.group.dev.ui.WebActivity" />
        <activity android:name="com.group.dev.ui.other.UCHomeActivity" />
        <activity android:name="com.group.dev.ui.sticky.StickyTop1Activity" />
        <activity android:name="com.group.dev.ui.other.ClipImageActivity" />
        <activity android:name="com.group.dev.ui.other.RulerActivity" />
        <activity android:name="com.group.dev.ui.clock.ClockActivity" />
        <activity android:name="com.group.dev.ui.other.TestActivity" />
        <activity android:name="com.group.dev.ui.flextag.FlexActivity" />
        <activity android:name="com.group.dev.ui.hook.TargetActivity" />
        <activity android:name="com.group.dev.ui.other.TextActivity" />
        <activity android:name="com.group.dev.ui.decoration_sticky.DecorationStickyActivity" />
        <activity android:name="com.group.dev.ui.tantan.TanTanActivity" />
        <activity android:name="com.group.dev.ui.fish.FishActivity" />
    </application>

</manifest>