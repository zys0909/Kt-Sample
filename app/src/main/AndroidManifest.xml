<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.dev.zhaoys">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-permission-sdk-23 android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/NoActionBar"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning"
        tools:targetApi="n">

        <activity-alias
            android:name=".app.App"
            android:icon="@color/colorAccent"
            android:targetActivity=".ui.home.HomeActivity"/>
        <activity android:name=".ui.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.wanandroid.WanMainActivity"/>
        <activity android:name=".ui.wanandroid.ArticleListActivity" />
        <activity android:name=".ui.WebActivity" />
        <activity android:name=".ui.other.UCHomeActivity" />
        <activity android:name=".ui.sticky.StickyRecyclerViewActivity" />
        <activity android:name=".ui.other.TestImageActivity" />
        <activity android:name=".ui.testview.TestViewActivity"/>
    </application>

</manifest>